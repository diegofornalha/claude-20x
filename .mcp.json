{
  "mcpServers": {
    "context7": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "@upstash/context7-mcp@latest"
      ]
    },
    "claude-flow": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "claude-flow@alpha",
        "mcp",
        "start"
      ]
    },
    "ruv-swarm": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "ruv-swarm@latest",
        "mcp",
        "start"
      ]
    },
    "neo4j-memory": {
      "type": "stdio",
      "command": "node",
      "args": [
        "/Users/agents/Desktop/claude-20x/.conductor/dalat/mcp-neo4j-agent-memory/build/index.js"
      ],
      "env": {
        "NEO4J_URI": "bolt://localhost:7687",
        "NEO4J_USERNAME": "neo4j",
        "NEO4J_PASSWORD": "claude-flow-2025",
        "NEO4J_DATABASE": "neo4j"
      }
    }
  }
}