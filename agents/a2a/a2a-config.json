{
  "a2a_configuration": {
    "enabled": true,
    "agent_name": "a2a_agent",
    "project_name": "a2a",
    "protocol_version": "1.0",
    "agent_type": "coordinator",
    "description": "Agent-to-Agent Communication and Coordination Hub",
    "discovery": {
      "auto_register": true,
      "registry_url": "http://localhost:8080/api/agents",
      "heartbeat_interval": 30000,
      "service_discovery": true,
      "announce_capabilities": true
    },
    "communication": {
      "transport": "http",
      "format": "json",
      "compression": false,
      "timeout": 30000,
      "retry_attempts": 3,
      "routing_enabled": true
    },
    "cooperation": {
      "task_delegation": true,
      "result_sharing": true,
      "skill_advertisement": true,
      "multi_agent_coordination": true,
      "message_routing": true,
      "capability_brokering": true
    },
    "security": {
      "authentication": false,
      "encryption": false,
      "rate_limiting": true,
      "message_validation": true,
      "agent_verification": false
    },
    "monitoring": {
      "metrics": true,
      "logging": true,
      "health_checks": true,
      "performance_tracking": true,
      "coordination_analytics": true
    },
    "capabilities": [
      "agent_discovery",
      "message_routing", 
      "multi_agent_coordination",
      "health_monitoring",
      "capability_management",
      "service_registry",
      "task_orchestration"
    ],
    "endpoints": {
      "health": "/health",
      "registry": "/registry", 
      "route": "/route",
      "coordinate": "/coordinate",
      "capabilities": "/capabilities",
      "discovery": "/discovery"
    },
    "registry": {
      "known_agents": {
        "guardian": {
          "url": "http://localhost:9999",
          "capabilities": ["sustainability", "monitoring", "health_check"],
          "priority": "high"
        },
        "helloworld": {
          "url": "http://localhost:9998", 
          "capabilities": ["hello", "basic_tasks"],
          "priority": "medium"
        },
        "marvin": {
          "url": "http://localhost:9997",
          "capabilities": ["extraction", "analysis", "marvin_tasks"], 
          "priority": "medium"
        }
      }
    },
    "coordination": {
      "max_concurrent_tasks": 10,
      "task_timeout": 300000,
      "coordination_patterns": [
        "sequential",
        "parallel", 
        "conditional",
        "pipeline"
      ]
    }
  }
}